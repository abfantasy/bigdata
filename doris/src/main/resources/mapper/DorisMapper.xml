<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.doris.mapper.DorisMapper">
    <select id="listDoris" resultType="com.example.doris.entity.DorisUser">
        select *
        from doris2_test
    </select>
    <select id="quryDoris" resultType="com.example.doris.entity.DorisUser">
        select *
        from doris2_test
        where id = #{id}
    </select>

    <update id="updateDoris">
        update doris2_test
        set name = #{name}
        where id = #{id}
    </update>
</mapper>
